{
  "name": "electron-vite-react",
  "productName": "MyVPN",
  "private": true,
  "version": "0.6.0",
  "description": "Vite React Electron boilerplate.",
  "author": "MyVPN",
  "license": "MIT",
  "main": "dist-electron/electron/main/index.js",
  "scripts": {
    "dev": "vite --mode mac",
    "dev:mac": "vite --mode mac",
    "dev:linux": "vite --mode linux",
    "dev:windows": "vite --mode windows",
    "dev:web": "vite --mode web",
    "build": "tsc && vite build --mode production.mac && electron-builder",
    "build:linux": "tsc && vite build --mode production.linux && electron-builder -l",
    "build:windows": "tsc && vite build --mode production.windows && electron-builder -w",
    "build:mac": "tsc && vite build --mode production.mac && electron-builder -m",
    "build:web": "PROD=true DEV=false tsc && vite build --mode production.web && electron-builder",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.js"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "classnames": "^2.3.2",
    "i18next": "^22.4.5",
    "react-i18next": "^12.1.1",
    "react-ionicons": "^4.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-react": "^2.2.0",
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "redux-logger": "^3.0.6",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-electron-plugin": "^0.5.2",
    "vite-plugin-electron-renderer": "^0.11.3"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777"
    }
  }
}
